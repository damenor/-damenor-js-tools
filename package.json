{
  "name": "@damenor/js-tools",
  "version": "0.0.1",
  "description": "Tools for Javascript",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "npm run build && npm pack",
    "addscope": "node tools/packagejson name @damenor/js-tools"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "tools",
    "helpers"
  ],
  "author": "David Menor <damenor.develop@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/damenor/-damenor-js-tools",
  "repository": {
    "type": "git",
    "url": "git@github.com:damenor/-damenor-js-tools.git"
  },
  "bugs": {
    "url": "https://github.com/damenor/-damenor-js-tools/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
